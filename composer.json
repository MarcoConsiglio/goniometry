{
    "name": "marcoconsiglio/goniometry",
    "description": "A PHP support for string, decimal, radian and object angles. Provides trigonometric algebra and congruent comparison.",
    "type": "library",
    "keywords": ["angle", "angle object", "goniometry", "angular value", "decimal angle", "radian angle", "radian", "degrees", "minutes", "seconds"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MarcoConsiglio\\Goniometry\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MarcoConsiglio\\Goniometry\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Marco Consiglio",
            "email": "mrccnsgl@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "fakerphp/faker": "~1.16",
        "phpdocumentor/shim": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "phpdocumentor/shim": true
        }
    }
}
